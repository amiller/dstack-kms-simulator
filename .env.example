# DStack KMS Simulator Environment Variables
# Copy this file to .env and fill in your values

# Private key for deploying contracts (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Phala Cloud API key for deployment
PHALA_CLOUD_API_KEY=phak_your_api_key_here

# RPC URL for Base network
RPC_URL=https://base.llamarpc.com

# Optional: Custom app ID for testing
DSTACK_APP_ID=your_app_id_here

# Optional: Custom verifier contract address  
VERIFIER_CONTRACT_ADDRESS=your_contract_address_here